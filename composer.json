{
    "name": "maxmumford/youtube-filter",
    "description": "Adds a twig filter that will transform regular YouTube watch URLs to embed URLs.",
    "version": "1.0.1",
    "type": "bolt-extension",
    "keywords": [
      "youtube",
      "filter",
      "embed",
      "media",
      "video"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
       "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Max Mumford",
            "email": "maxmumford@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\MaxMumford\\YouTubeFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\MaxMumford\\YouTubeFilter\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\MaxMumford\\YouTubeFilter\\YouTubeFilterExtension"
    }
}
